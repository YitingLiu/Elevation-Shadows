{
  "name": "ElevationShadows",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "ElevationShadows",
    "manifest": "src/manifest.json",
    "main": "Elevation-Shadows.sketchplugin",
