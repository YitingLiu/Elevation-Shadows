  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Yiting Liu <yl3799@nyu.edu>",
  "repository": {
    "type": "git",
    "url": "https://github.com/YitingLiu/Elevation-Shadows"
  }
